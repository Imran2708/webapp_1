{
  "routes": {
    "/api/detectLanguage": "api/detectLanguage",
    "/api/getIceServerToken": "api/getIceServerToken",
    "/api/getSpeechToken": "api/getSpeechToken",
    "/api/message": "api/message",
    "/*": "src/index.html"
    "route": "/*"
    "allowedRoles": ["anonymous", "authenticated"],
    "methods": ["GET", "POST", "PUT", "OPTIONS"]
  },
  "navigationFallback": {
    "rewrite": "src/index.html",
    "exclude": ["/api/*"]
  }
}
